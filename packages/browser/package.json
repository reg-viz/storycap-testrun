{
  "name": "@storycap-testrun/browser",
  "version": "2.0.0",
  "description": "Screenshot capture for Storybook addon-vitest - Provides stable visual testing functionality and Vitest plugin for Storybook stories running in browser environments",
  "keywords": [
    "storybook",
    "storybook-addon",
    "storycap",
    "addon",
    "addon-vitest",
    "playwright",
    "screenshot",
    "visual-testing",
    "testing",
    "test",
    "vitest"
  ],
  "homepage": "https://github.com/reg-viz/storycap-testrun",
  "bugs": {
    "url": "https://github.com/reg-viz/storycap-testrun/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reg-viz/storycap-testrun.git",
    "directory": "packages/browser"
  },
  "license": "MIT",
  "author": "wadackel",
  "contributors": [
    "Yosuke Kurami"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vitest-plugin": {
      "types": "./dist/vitest-plugin/index.d.ts",
      "require": "./dist/vitest-plugin/index.js",
      "import": "./dist/vitest-plugin/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@storycap-testrun/internal": "workspace:*"
  },
  "devDependencies": {
    "@vitest/browser": "4.0.3",
    "vitest": "4.0.3"
  },
  "peerDependencies": {
    "@vitest/browser": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "tsdown": {
    "entry": [
      "src/index.ts",
      "src/vitest-plugin/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "splitting": false,
    "sourcemap": false,
    "clean": true,
    "minify": true,
    "dts": true
  }
}
