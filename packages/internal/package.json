{
  "name": "@storycap-testrun/internal",
  "version": "2.0.0",
  "description": "Core utilities and shared types for storycap-testrun - Provides screenshot options, stability checks, hook processing, and common functionality for browser and Node.js environments",
  "keywords": [
    "storybook",
    "storycap"
  ],
  "homepage": "https://github.com/reg-viz/storycap-testrun",
  "bugs": {
    "url": "https://github.com/reg-viz/storycap-testrun/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reg-viz/storycap-testrun.git",
    "directory": "packages/internal"
  },
  "license": "MIT",
  "author": "wadackel",
  "contributors": [
    "Yosuke Kurami"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc"
  },
  "tsdown": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "splitting": false,
    "sourcemap": false,
    "clean": true,
    "minify": true,
    "dts": true
  }
}
